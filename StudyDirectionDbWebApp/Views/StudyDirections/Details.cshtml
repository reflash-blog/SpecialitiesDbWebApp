@model StudyDirectionDbWebApp.Models.StudyDirection

@{
    ViewBag.Title = "Подробнее";
}


<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Название")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StudyDirectionName)
        </dd>

        <dt>
            @Html.DisplayName("Специальность")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Speciality.SpecialityName)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Изменить", "Edit", new { id = Model.DirectionId }, new { @class = "btn btn-default" })
    @Html.ActionLink("Назад", "Index", null, new { @class = "btn btn-primary" })
</p>
